import java.util.ArrayList; // import the ArrayList class

public class Resident {
    
        private int id;
        private String Nom ;
        private String Mdp ;
        
        
        
        
        public Resident (int id,String Nom,String Mdp) {
            this.id=id;
            this.Nom=Nom;
            this.Mdp=Mdp;
            
        
        }

    //------------------- Methode
    

    //--------- getters
    
    
    public int getid() {
        return id;
    } 
    public int getNom() {
        return Nom;
    } 
    public int getMdp() {
        return Mdp;
    } 

   
    //--------- setters
    
        
        
      

       
       
}
